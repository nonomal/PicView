            Margin="7,0"
            Maximum="100"
            Minimum="0"
            Value="0"
            x:Name="ProgressBar" />

        <StackPanel
            HorizontalAlignment="Center"
            Margin="0,10"
            Orientation="Horizontal">
            <Button
                Classes="BorderStyle altHover mainBtn"
                CornerRadius="4"
                x:Name="CancelButton">
                <TextBlock
                    Classes="txt"
                    Padding="15,0"
                    Text="{CompiledBinding Reset,
                                           Mode=OneWay}"
                    TextAlignment="Center"
                    x:Name="CancelButtonTextBlock" />
            </Button>

            <Button
                Background="{DynamicResource AccentColor}"
                Classes="BorderStyle accentHover mainBtn"
                CornerRadius="4"
                IsEnabled="False"
                x:Name="StartButton">
                <TextBlock
                    Classes="txt"
                    Padding="15,0"
                    Text="{CompiledBinding Start,
                                           Mode=OneWay}"
                    TextAlignment="Center" />
            </Button>
        </StackPanel>

    </StackPanel>


</UserControl>
